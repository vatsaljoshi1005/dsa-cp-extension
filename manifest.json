{
  "manifest_version": 3,
  "name": "DSA & CP Recommender",
  "version": "1.0",
  "description": "Suggests 1 question each from LeetCode, Codeforces, and CodeChef.",
  "action": {
    "default_popup": "popup.html"
    
  },
  "background":{
    "service_worker": "background.js"
  },
  "permissions": ["tabs","storage"],
  "host_permissions": [
    "https://codeforces.com/*",
    "https://leetcode.com/*",
    "https://www.codechef.com/*"
  ]
}
